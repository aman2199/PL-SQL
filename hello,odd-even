BEGIN
dbms_output.put_line('Hello');
END;
/


DECLARE
name varchar(20):='HELLO';
BEGIN
dbms_output.put_line(name);
END;
/

DECLARE
x number(10):= 5;
y number(10):= 6;
z number(10);
BEGIN
z:=x+y;
dbms_output.put_line('The sum of numbers is '||z);
END;
/

DECLARE
x number :=20;
r number;
BEGIN
r:= MOD(x,2);
if r=0 then
dbms_output.put_line('The number is Even');
else
dbms_output.put_line('The Number is Odd');
END IF;
END;
/
